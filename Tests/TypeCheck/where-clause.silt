module WhereClause where

flip : {A B : Type} {C : A -> B -> Type} ->
       ((x : A) (y : B) -> C x y) -> ((y : B) (x : A) -> C x y)
flip = ride where
  ride : {A B : Type} {C : A -> B -> Type} -> ((x : A) (y : B) -> C x y) -> ((y : B) (x : A) -> C x y)
  ride f y x = f x y

